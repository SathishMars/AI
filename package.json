{
  "name": "groupize-workflows",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_EXTRA_CA_CERTS=\"./local-dev/nginx/certs/mkcert-ca-cert.pem\" PORT=3000 next dev",
    "dev:insights": "cross-env NODE_OPTIONS=--max-old-space-size=8192 PORT=3000 next dev",
    "dev:local-rails": "cross-env PORT=3001 next dev",
    "build": "next build",
    "start": "PORT=3001 next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npm rebuild lightningcss --silent || true",
    "docker-build": "docker build -t groupize-workflows .",
    "mongodb": "bash local-dev/mongodb/mongodb.sh",
    "nginx": "bash local-dev/nginx/start.sh",
    "nginx:local-rails": "bash local-dev/nginx-local-rails/start.sh",
    "setup:remote-groupize": "bash local-dev/remote-groupize-setup.sh",
    "setup:local-rails:init": "bash local-dev/local-groupize-hosts-setup.sh && npm run nginx:local-rails",
    "setup:local-rails:cleanup": "npm run nginx:local-rails stop; bash local-dev/local-groupize-hosts-cleanup.sh",
    "cleanup:remote-groupize": "bash local-dev/remote-groupize-cleanup.sh"
  },
